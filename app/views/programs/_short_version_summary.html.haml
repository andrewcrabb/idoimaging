%table.table
  %thead
    %tr
      %th Name
      %th Summary
      %th Version

  %tbody
    - versions.each do |version|
      - program = version.program
      %tr
        %td= link_to(program.name, program_path(program))
        %td= program.summary
        %td.white-space-pre
          = version.version
          %br
          = version.date
